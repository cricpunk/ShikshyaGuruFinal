<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:id="@+id/d_circle_inner"
        android:bottom="3dp"
        android:left="3dp"
        android:right="3dp"
        android:top="3dp">
        <shape android:shape="oval">
            <solid android:color="#FAFAFA" />
        </shape>
    </item>
    <item android:id="@+id/d_circle_outer">
        <shape android:shape="oval">
            <stroke
                android:width="1dp"
                android:color="#FAFAFA" />
        </shape>
    </item>
</layer-list>